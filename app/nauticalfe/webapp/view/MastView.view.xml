<mvc:View controllerName="nauticalfe.controller.MastView"
xmlns:html="http://www.w3.org/1999/xhtml"
xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:core="sap.ui.core"
xmlns="sap.m" xmlns:tnt="sap.tnt">
 
    <Page id="_IDGenPage1" class="page">
       
        <headerContent>
            <!-- Your header content goes here -->
            <Bar id="_IDGenBar1" class="customHeader">
                <contentLeft>
                    <core:Icon id="_IDGenIcon1"
                        src="sap-icon://nav-back"
                        class="size1"
                        color="#FFFFFF"
                        press="onBackPress"
                    />
                    <core:Icon id="_IDGenIcon2"
                        src="sap-icon://home"
                        class="size1"
                        color="#FFFFFF"
                        press="onBackPressHome"
                    />
                    <Image
                         class="size1"
                        src="./images/ignxlogo.png"
                        height="75%" activeSrc="https://www.ingenxtec.com"
                   
                    />
                    <Text id="_IDGenText1"
                        class="textBid size1"
                        text="Master Data Dashboard"
                    />
                </contentLeft>
                <contentRight>
                    <core:Icon id="_IDGenIcon3"
                        src="sap-icon://search"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData id="_IDGenFlexItemData1" growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                    <core:Icon id="_IDGenIcon4"
                        src="sap-icon://SAP-icons-TNT/user"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData id="_IDGenFlexItemData2" growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                </contentRight>
            </Bar>
        </headerContent>
 
        <VBox id="_IDGenVBox1"  class="mastDash" >
            <Title text="Selection Option" titleStyle="H4" />
            <FlexBox class="masterFlexBox">
                <GenericTile id="GenGenericTile1" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Voyage Type" press="navToVoyageType" >
                    <TileContent id="_IDGenTileContent1">
                    <ImageContent id="_IDGenImageContent1" src="./images/voyage.png" ></ImageContent>
                    </TileContent>
                </GenericTile>
                <GenericTile id="GenGenericTile2" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Vessel Type" press="navToVesselType">
                    <TileContent >
                    <ImageContent src="./images/vessel.png"></ImageContent>
                    </TileContent>
                </GenericTile>
                <GenericTile id="GenGenericTile3" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Currency" press="navToCurrencyType">
                    <TileContent >
                    <ImageContent src="./images/currency.png"></ImageContent>
                    </TileContent>
                </GenericTile>
                <GenericTile id="GenGenericTile4" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Class Master" press="navToClassMaster">
                    <TileContent >
                    <ImageContent src="./images/class.png"></ImageContent>
                    </TileContent>
                </GenericTile>
                <GenericTile id="GenGenericTile5" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Bid Master" press="onBidMaster">
                    <TileContent >
                    <ImageContent src="./images/bidmaster.png"></ImageContent>
                    </TileContent>
                </GenericTile>
                    <GenericTile id="GenGenericTile6" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Port/Loc Master" press="onPortLoc">
                    <TileContent >
                    <ImageContent src="./images/portLocMaster.png"></ImageContent>
                    </TileContent>
                </GenericTile>
                <GenericTile id="GenGenericTile7" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Route Master" press="onRouterMaster">
                    <TileContent >
                    <ImageContent src="./images/route.png"></ImageContent>
                    </TileContent>
                </GenericTile>
                
            </FlexBox>
            <FlexBox class="masterFlexBox">
                <GenericTile id="GenGenericTile8" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="UOM" press="onUoM">
                    <TileContent >
                    <ImageContent src="./images/uom.png"></ImageContent>
                    </TileContent>
                </GenericTile>
                <GenericTile id="GenGenericTile9" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Cost Master" press="onCostMaster">
                    <TileContent >
                    <ImageContent src="./images/cost.png"></ImageContent>
                    </TileContent>
                </GenericTile>
                <GenericTile id="GenGenericTile10" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Event Master" press="onEventMaster">
                </GenericTile>
                <GenericTile id="GenGenericTile11" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Ref. Doc. Indicator" press="onRefDocIndicator">
                </GenericTile>
                <GenericTile id="GenGenericTile12" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Business Partner" press="onBusinessPartnerPress">
                </GenericTile>
                <GenericTile id="GenGenericTile13" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Configure Release" press="onConfigPress">
                </GenericTile>
                <GenericTile id="GenGenericTile14" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Port/Loc Upload" press="onPortLocUpload">
                </GenericTile>
                
            </FlexBox>
            <FlexBox class="masterFlexBox">
                <GenericTile id="GenGenericTile15" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Port Master" press="onPortMaster">
                </GenericTile>
                <GenericTile id="GenGenericTile16" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Port Upd" press="onPortUpload">
                </GenericTile>
                <GenericTile id="GenGenericTile17" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Country Master Upd" press="onCountryMasterUpd">
                </GenericTile>
                <GenericTile id="GenGenericTile18" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Country Master" press="onCountryMaster">
                </GenericTile>
                <GenericTile id="GenGenericTile19" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="API URL" press="onApiurl">
                </GenericTile>
                <GenericTile id="GenGenericTile20" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Marine Path Upd" press="onMarinePathUpd">
                </GenericTile>
                <GenericTile id="GenGenericTile21" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Marine Dis Upd" press="onMarineDisUpd">
                </GenericTile>
                
            </FlexBox>
        </VBox>

        
     
    </Page>
</mvc:View>