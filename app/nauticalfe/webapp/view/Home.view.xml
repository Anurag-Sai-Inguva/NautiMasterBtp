
<mvc:View xmlns:tnt="sap.tnt" xmlns:cards="sap.f.cards"
xmlns:form="sap.ui.layout.form"
 controllerName="nauticalfe.controller.Home" xmlns:core="sap.ui.core"
    xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">

    <Page id="page" class="page">

        <headerContent>
            <!-- Your header content goes here -->
            <Bar class="customHeader">
                <contentLeft>
                    <Image
                         class="size1"
                        src="./images/ignxlogo.png"
                        height="75%"
                    />
                    <Text
                        class="textBid size1"
                        text="Home"
                    />
                </contentLeft>
                <contentRight>
                    <core:Icon
                        src="sap-icon://search"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                    <core:Icon
                        src="sap-icon://SAP-icons-TNT/user"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                </contentRight>
            </Bar>
        </headerContent>


        <GenericTile id="_IDGenGenericTile1" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
         header="Nautical" subheader="Create Voyage" press=".onPress1" >
            <TileContent id="_IDGenTileContent1">
            <ImageContent id="_IDGenImageContent1" src="./images/voyage.png"></ImageContent>
            </TileContent>
        </GenericTile>
    
        <GenericTile id="_IDGenGenericTile2" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
         header="Nautical" subheader="Live Bidding Controller" press=".onPress2">
            <TileContent id="_IDGenTileContent2" >
            <ImageContent id="IDGenImageContent1" src="./images/compareReqForQuotation.png"></ImageContent>
            </TileContent>
        </GenericTile>

        <GenericTile id="GenGenericTile23" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
         header="Transaction" subheader="Dashboard" press=".onPress3">
            <TileContent id="GenTileContent4">
            <ImageContent id="GenImageContent3" src="./images/voyage.png"></ImageContent>
            </TileContent>
        </GenericTile>

        <GenericTile id="GenGenericTile12" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
         header="Master" subheader="Dashboard" press=".onPress4">
            <TileContent id="GenTileContent5">
            <ImageContent id="GenImageContent4" src="./images/voyage.png"></ImageContent>
            </TileContent>
        </GenericTile>
 

    </Page>
       
</mvc:View>
 
 
 
 
 
 
 
 
 