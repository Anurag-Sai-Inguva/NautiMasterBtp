<mvc:View controllerName="nauticalfe.controller.BusinessPartner"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
     xmlns:core="sap.ui.core">
    <Page id="businessPartnerPageId" >
       <headerContent>
            <!-- Your header content goes here -->
            <Bar
                id="_IDGenBar1"
                class="customHeader"
            >
                <contentLeft>
                    <core:Icon
                        id="_IDGenIcon1"
                        src="sap-icon://nav-back"
                        class="size1"
                        color="#FFFFFF"
                        press="backPress"
                    />
                    <core:Icon
                        id="_IDGenIcon2"
                        src="sap-icon://home"
                        class="size1"
                        color="#FFFFFF"
                        press="onBackPressHome"
                    />
                    <Image
                        id="_IDGenImage2"
                        src="../images/ingenxlogo.jpg"
                        height="75%"
                    />
                    <Text
                        id="_IDGenText1"
                        class="textBid"
                        text='Business Partner'
                    />
                </contentLeft>
                <contentRight>
                    <core:Icon
                        id="_IDGIcon3"
                        src="sap-icon://search"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData
                                id="_IDGenFlexItemData1"
                                growFactor="1"
                            />
                        </core:layoutData>
                    </core:Icon>
                    <core:Icon
                        id="_IDGenIcon4"
                        src="sap-icon://person-placeholder"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData
                                id="_IDGenFlexItemData2"
                                growFactor="1"
                            />
                        </core:layoutData>
                    </core:Icon>
                </contentRight>
            </Bar>
        </headerContent>
            <VBox id="_IDGenVBox1" class="vPanel">


                <Panel id="_IDGenPanel1" class="panel" width="700px" height="500px">
                    <ObjectListItem id="_IDGenObjectListItem1" intro="Select Options" ></ObjectListItem>

                    <Button id="_IDGenButton1" text="BP Master Details" class="Trans" width="30%"
                    icon="sap-icon://request" press="onBPDetailpress"></Button>

                    <Button id="_IDGenButton2" text="Vendor Data Syncing" press="onVendorDataPress" class="Mastr" width="30%" icon="sap-icon://visits"></Button>
                
                </Panel> 

            </VBox>
         <footer>
        <OverflowToolbar ></OverflowToolbar>
        </footer>
    </Page>
</mvc:View>