<mvc:View xmlns:tnt="sap.tnt"
xmlns:form="sap.ui.layout.form"
xmlns:l="sap.ui.layout"
controllerName="nauticalfe.controller.ConfigReleaseStrategy"
xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
xmlns:core="sap.ui.core">
    
   <Page id="_IDGenPage1"  >
      <headerContent>
            <!-- Your header content goes here -->
            <Bar
                id="_IDGenBar1"
                class="customHeader"
            >
                <contentLeft>
                    <core:Icon
                        id="_IDGenIcon1"
                        src="sap-icon://nav-back"
                        class="size1"
                        color="#FFFFFF"
                        press="backPress"
                    />
                    <core:Icon
                        id="_IDGenIcon2"
                        src="sap-icon://home"
                        class="size1"
                        color="#FFFFFF"
                        press="onBackPressHome"
                    />
                    <Image
                        id="_IDGenImage2"
                        src="./images/ignxlogo.png"
                        height="75%"
                    />
                    <Text
                        id="_IDGenText1"
                        class="textBid size1"
                        text='Config Release'
                    />
                </contentLeft>
                <contentRight>
                    <core:Icon
                        id="_IDGIcon3"
                        src="sap-icon://search"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData
                                id="_IDGenFlexItemData1"
                                growFactor="1"
                            />
                        </core:layoutData>
                    </core:Icon>
                    <core:Icon
                        id="_IDGenIcon4"
                        src="sap-icon://person-placeholder"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData
                                id="_IDGenFlexItemData2"
                                growFactor="1"
                            />
                        </core:layoutData>
                    </core:Icon>
                </contentRight>
            </Bar>
        </headerContent>
        <Text text="Selection Option" id="HeadOptions" class="configHeading "/>
      
        <VBox id="VoyageOperations" >
            <Button id="_IDGenButton1" text="Maintain Group" width="300px" type="Reject" class="mgbtn" icon="sap-icon://calendar" press="onMaintainGroupPress"></Button>
            <Button id="_IDGenButton2" text="Voyage" width="300px" type="Reject" class="mgbtn" icon="../images/pen_msg.png" press="onConfigVoyage"></Button>
            <Button id="_IDGenButton3" text="Chatering" width="300px" type="Reject" class="mgbtn" press="onConfigChartering"></Button>
        </VBox>

        <footer>
            <OverflowToolbar ></OverflowToolbar>
        </footer>

   </Page>
   
    

</mvc:View> 